import java.util.Arrays;
public class array4
{
	public static void main(String[] args)
	{
		int a[]= {2000,1989,1980,2002};
		String[]s= {"joy","ram","shyam","riya"};
		System.out.println("Integer array is "+ Arrays.toString(a));
		Arrays.sort(a);
		System.out.println("sorted Int array"+ Arrays.toString(a));
		Arrays.sort(s);
		System.out.println("String array is"+Arrays.toString(s));
			System.out.println("sorted Int array"+ Arrays.toString(s));
	}
}
